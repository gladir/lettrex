Var
 I,J:Integer;
 CurrName:String;

Function StrToUpper(S:String):String;
Var
 I:Byte;
Begin
 For I:=1 to Length(S)do Begin
  If S[I] in['a'..'z']Then S[I]:=Chr(Ord(S[I])-32);
 End;
 StrToUpper:=S;
End;

BEGIN
  If(ParamStr(1)='/?')or(ParamStr(1)='--help')or(ParamStr(1)='-h')or
   (ParamStr(1)='/h')or(ParamStr(1)='/H')Then Begin
  WriteLn('ABBREV : Cette commande permet d''indiquer le nom complet ',
                      'd''une abrÇviation.');
  WriteLn;
  WriteLn('Syntaxe : ABBREV nom');
  WriteLn;
 End
  Else
 If ParamCount>0 Then Begin
  For I:=1 to ParamCount do Begin
   CurrName:=ParamStr(I);
   For J:=Low(AbbreviationList)to High(AbbreviationList)do Begin
    If StrToUpper(CurrName)=StrToUpper(StrPas(AbbreviationList[J].Abbr))Then Begin
     WriteLn('AbrÇviation: ',StrPas(AbbreviationList[J].Abbr));
     WriteLn('Signification : ',StrPas(AbbreviationList[J].Def));
     WriteLn;
    End;
   End;
  End;
 End
  Else
 WriteLn('Paramätre requis !');
END.
